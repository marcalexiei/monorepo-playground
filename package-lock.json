{
  "name": "monorepo-playground",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "monorepo-playground",
      "version": "1.0.0",
      "license": "MIT",
      "workspaces": [
        "packages/*",
        "apps/*"
      ],
      "devDependencies": {
        "typescript": "5.3.3"
      }
    },
    "apps/dashboard": {
      "version": "1.0.0",
      "license": "MIT",
      "dependencies": {
        "@monorepoplayground/models": "1.0.0"
      },
      "devDependencies": {}
    },
    "apps/login": {
      "version": "1.0.0",
      "extraneous": true,
      "license": "MIT",
      "devDependencies": {}
    },
    "node_modules/@monorepoplayground/components": {
      "resolved": "packages/components",
      "link": true
    },
    "node_modules/@monorepoplayground/models": {
      "resolved": "packages/models",
      "link": true
    },
    "node_modules/dashboard": {
      "resolved": "apps/dashboard",
      "link": true
    },
    "node_modules/typescript": {
      "version": "5.3.3",
      "resolved": "https://registry.npmjs.org/typescript/-/typescript-5.3.3.tgz",
      "integrity": "sha512-pXWcraxM0uxAS+tN0AG/BF2TyqmHO014Z070UsJ+pFvYuRSq8KH8DmWpnbXe0pEPDHXZV3FcAbJkijJ5oNEnWw==",
      "dev": true,
      "bin": {
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": {
        "node": ">=14.17"
      }
    },
    "packages/components": {
      "name": "@monorepoplayground/components",
      "version": "1.0.0",
      "license": "MIT"
    },
    "packages/models": {
      "name": "@monorepoplayground/models",
      "version": "1.0.0",
      "license": "MIT"
    }
  }
}
